        "hero": {
          "template": "hero",
          "title": "Welcome to FE Template!",
          "description": "My go-to starting point for front-end projects. Dive in and explore the <b>meticulously crafted folder structure</b> and <b>components</b>, meticulously designed using Shadcn/UI best practices. Start building your next masterpiece with <b>ease</b> and <b>efficiency</b>!",
          "action": {
            "label": "Go to github",
            "href": "https://github.com/Richard-Passos/fe-template",
            "isExternal": "true",
            "color": "primary",
            "type": "solid",
            "size": "md"
          },
          "scroll": {
            "icon": "/images/caret-down.svg",
            "description": "Scroll to see more"
          },
          "block": {
            "slug": "components-nav",
            "title": "Amazing components"
          }
        },
        "whyUs": {
          "template": "one-column",
          "type": "pattern",
          "title": "Why FE template?",
          "block": {
            "slug": "why-us",
            "items": {
              "0": {
                "color": "muted",
                "title": "Saves time",
                "description": "No need to spend hours implementing all over again. Get it right away!",
                "icon": "/images/stopwatch.svg"
              },
              "1": {
                "color": "muted",
                "title": "It’s realistic",
                "description": "Get internalization, templates, and a easy to customize code.",
                "icon": "/images/globe.svg"
              },
              "2": {
                "color": "muted",
                "title": "It’s simple",
                "description": "Clone it via Github, and remove what you don't want.",
                "icon": "/images/check-badge.svg"
              }
            }
          }
        },
        "about": {
          "template": "one-column",
          "block": {
            "slug": "bento-grid",
            "templates": {
              "default": "'item-0' 'item-1' 'item-2' 'item-3'",
